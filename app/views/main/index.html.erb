<h1>hi</h1>

<% if user_signed_in? %>
	  <h1>User is LOGGED IN! :)</h1>
<% else %>
	  <h1>User is NOT logged in! :(</h1>
<% end %>

<% if current_user %>
  <h3><%= current_user.name %></h3>
  <h3><%= current_user.email %></h3>
  <%= image_tag(current_user.image) unless current_user.image.nil? %>
<% end %>
